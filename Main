//MAIN ROUTINE


void setup() {
  //Library calls
  #include Wire.h
  
  //Define Constants: i.e. I2C address
  accel_data = 00000110 //Pin 6 on MPU6050 - AUX_DA Pin
  accel_clk = 00000111 //Pin 7 - AUX_CL Pin
  
  //Establish Connection and Configure MPU6050 Settings
  Wire.beginTransmission();
  
  
}

void loop() {
  while(!imu_read());
  rc_read_values();
  fc_process();
}

